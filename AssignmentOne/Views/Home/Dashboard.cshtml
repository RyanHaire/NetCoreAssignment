@{
    Layout = null;
    var data = ViewBag.User;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/lib/bootstrap/dist/css/bootstrap.css">
    <title>Dashboard</title>
</head>
<body class="text-center">
     <h5 class="bg-primary text-white text-center m-2 p-2">Dashboard</h5>
    <p>Welcome to your dashboard @data.FirstName!</p>
    <p>Email: @data.Email</p> 
    <div>
        <p>
            <a class="btn btn-primary" asp-action="UserListing">User Listing</a>
        </p>
        
        <p>
            @Html.ActionLink("Sign Out", "SignOut", "Home", new { Email = data.Email}, new {@class = "btn btn-secondary"})
        </p>
    </div>
</body>
</html>